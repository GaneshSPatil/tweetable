<div class="tab-pane">
  <div class="row">
    <div class="col-md-12">
      <div class="panel-group" id="panel-982585">

        <% filtered_passages.each do |passage_with_response| %>
            <div class="panel panel-default">
              <div class="panel-heading">
                <a class="panel-title" data-toggle="collapse" data-parent="#panel-982585" href="#panel-element-<%= passage_with_response[:passage].id %>">
                  <div class="row">
                    <div class="col-md-8">
                      <span><%= passage_with_response[:passage].title %></span>
                    </div>
                    <div class="col-md-2 pull-right">
                      <span class="pull-right">
                        <%= to_display_time_format passage_with_response[:passage].close_time%>
                      </span>
                    </div>
                  </div>
                </a>
              </div>
              <div id="panel-element-<%= passage_with_response[:passage].id %>" class="panel-collapse collapse">
                <div class="panel-body">
                  <div class="row">
                    <%= render "passages/candidate/attempted_passages", :passage_with_response => passage_with_response %>
                  </div>
                </div>
              </div>
            </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
