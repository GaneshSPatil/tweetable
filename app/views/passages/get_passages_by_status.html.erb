<div class="tab-pane">
  <div class="row">
    <div class="col-md-12">
      <div class="panel-group" id="panel-982585">

        <% @filtered_passages.each do |passage| %>
            <div class="panel panel-default">
              <div class="panel-heading">
                <a class="panel-title" data-toggle="collapse" data-parent="#panel-982585" href="#panel-element-<%= passage.id %>">
                  <div class="row">
                    <div class="col-md-8">
                      <span><%= passage.title %></span>
                    </div>
                    <div class="col-md-1 pull-right">
                      <!--<a class="fa fa-eye" style="padding-right: 20px" aria-hidden="true"></a>-->
                      <a class="fa fa-pencil" style="padding-right: 20px" aria-hidden="true"></a>
                      <a class="fa fa-trash" aria-hidden="true"></a>
                    </div>
                  </div>
                </a>
              </div>
              <div id="panel-element-<%= passage.id %>" class="panel-collapse collapse">
                <div class="panel-body">
                  <div class="row">
                    <%= render "passages/admin/" + @status.downcase + "_passages", :passage => passage %>
                  </div>
                </div>
              </div>
            </div>
        <% end %>
      </div>
    </div>
  </div>
</div>